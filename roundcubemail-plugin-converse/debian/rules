#!/usr/bin/make -f
# Uncomment this to turn on verbose mode.
export DH_VERBOSE=1

%:
	dh $@

override_dh_install:
	dh_install
	mv $(CURDIR)/debian/roundcubemail-plugin-converse/etc/roundcubemail/config.inc.php.dist \
		$(CURDIR)/debian/roundcubemail-plugin-converse/etc/roundcubemail/converse.inc.php

